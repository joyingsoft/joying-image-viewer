{
  "name": "joyview",
  "version": "1.0.0",
  "description": "View local images, photos, gifs in browser.",
  "main": "index.tsx",
  "scripts": {
    "watch": "webpack --watch",
    "dev": "webpack serve --open --config webpack.dev.js",
    "build": "webpack  --config webpack.prod.js",
    "prettier": "prettier --config .prettierrc --check \"src/**/*.{js,jsx,ts,tsx,scss,css,md}\"",
    "prettier:fix": "prettier --config .prettierrc --write \"src/**/*.{js,jsx,ts,tsx,scss,css,md}\"",
    "lint": "eslint --ext .js,.ts,.tsx src/",
    "lint:fix": "eslint --ext .js,.ts,.tsx --fix src/",
    "verify": "npm run lint & npm run prettier",
    "fix": "npm run lint:fix & npm run prettier:fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "debug": "node-nightly --inspect ./node_modules/webpack/bin/webpack.js --mode=development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joyingsoft/joyview.git"
  },
  "keywords": [
    "image",
    "photo",
    "picture",
    "gif",
    "view",
    "viewer",
    "browser"
  ],
  "author": "Junjun Guo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joyingsoft/joyview/issues"
  },
  "homepage": "https://github.com/joyingsoft/joyview#readme",
  "devDependencies": {
    "@iconify/react": "^3.2.1",
    "@types/eslint": "^8.4.1",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/wicg-file-system-access": "^2020.9.5",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@typescript-eslint/typescript-estree": "^5.17.0",
    "assets-webpack-plugin": "^7.1.1",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-typescript": "^16.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "html-minimizer-webpack-plugin": "^3.5.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "prettier": "^2.6.2",
    "sass": "^1.49.11",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "browser-fs-access": "^0.25.3",
    "i18next": "^21.6.14",
    "i18next-browser-languagedetector": "^6.1.4",
    "i18next-http-backend": "^1.4.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-i18next": "^11.16.2",
    "react-router-dom": "^6.3.0"
  }
}
