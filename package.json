{
  "name": "joyview",
  "version": "1.0.0",
  "description": "View local images, photos, gifs in browser.",
  "main": "index.tsx",
  "scripts": {
    "watch": "webpack --watch",
    "dev": "webpack serve --open --config webpack.dev.js",
    "build": "webpack  --config webpack.prod.js",
    "prettier": "prettier --config .prettierrc --check \"src/**/*.{js,jsx,ts,tsx,scss,css,md}\"",
    "prettier:fix": "prettier --config .prettierrc --write \"src/**/*.{js,jsx,ts,tsx,scss,css,md}\"",
    "lint": "eslint --ext .js,.ts,.tsx src/",
    "lint:fix": "eslint --ext .js,.ts,.tsx --fix src/",
    "verify": "npm run lint & npm run prettier",
    "fix": "npm run lint:fix & npm run prettier:fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "debug": "node-nightly --inspect ./node_modules/webpack/bin/webpack.js --mode=development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joyingsoft/joyview.git"
  },
  "keywords": [
    "image",
    "photo",
    "picture",
    "gif",
    "view",
    "viewer",
    "browser"
  ],
  "author": "Junjun Guo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joyingsoft/joyview/issues"
  },
  "homepage": "https://github.com/joyingsoft/joyview#readme",
  "devDependencies": {
    "@types/eslint": "^8.2.1",
    "@types/node": "^17.0.0",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/wicg-file-system-access": "^2020.9.4",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "@typescript-eslint/typescript-estree": "^5.7.0",
    "assets-webpack-plugin": "^7.1.1",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "html-minimizer-webpack-plugin": "^3.5.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.5",
    "prettier": "^2.5.1",
    "sass": "^1.45.0",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "browser-fs-access": "^0.23.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.1.1"
  }
}
